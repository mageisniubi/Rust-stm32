/*导入符号main*/
EXTERN(main)
/*使用main函数作为程序入口*/
ENTRY(main)
/*内存分配*/
SECTIONS
{
    /*从0x08000000开始分配,.是分配器*/
    . = 0x08000000;
    .text :
    {
        /*ram的首地址和大小，会把第一个4字节里面的值作为栈底*/
        LONG(0x20000000 + 192 * 1024);
        /*会把第二个4字节里面的值放入pc寄存器，所以要放main的地址*/
        LONG(main);
        /*中断向量表，最多256个字节，就是256个中断*/
        . = 0x08000400-8;
        /*以下存放代码*/
        *(.text)
    }
}
